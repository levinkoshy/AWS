ssh ec2-user@52.56.53.250 -i MyEC2KeyPair.pem

sudo su
//elevate privileges to root


yum update -y
//update the kernal, security groups update


yum install httpd -y
//install Apache

service httpd start
//Start Apache server

chkconfig httpd on
// make sure that Apache server starts everytime we rebuild

cd /var/www/html

ls

nano index.html 